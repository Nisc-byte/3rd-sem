#include <iostream>
using namespace std;

#define MAX 5

class StaticQueue {
    int q[MAX], f, r;
public:
    StaticQueue() { f = r = -1; }

    void enqueue(int x) {
        if (r == MAX - 1)
            cout << "Overflow\n";
        else {
            if (f == -1) f = 0;
            q[++r] = x;
            cout << "Inserted\n";
        }
    }

    void dequeue() {
        if (f == -1 || f > r)
            cout << "Underflow\n";
        else
            cout << "Deleted: " << q[f++] << endl;
    }

    void display() {
        if (f == -1 || f > r)
            cout << "Empty\n";
        else {
            for (int i = f; i <= r; i++)
                cout << q[i] << " ";
            cout << endl;
        }
    }

    void makeEmpty() {
        f = r = -1;
        cout << "Queue Emptied\n";
    }
};

class PriorityQueue {
    int q[MAX], rear;
public:
    PriorityQueue() { rear = -1; }

    void enqueue(int x) {
        if (rear == MAX - 1)
            cout << "Overflow\n";
        else {
            int i;
            for (i = rear; i >= 0 && q[i] > x; i--)
                q[i + 1] = q[i];
            q[i + 1] = x;
            rear++;
            cout << "Inserted\n";
        }
    }

    void dequeue() {
        if (rear == -1)
            cout << "Underflow\n";
        else {
            cout << "Deleted: " << q[0] << endl;
            for (int i = 0; i < rear; i++)
                q[i] = q[i + 1];
            rear--;
        }
    }

    void display() {
        if (rear == -1)
            cout << "Empty\n";
        else {
            for (int i = 0; i <= rear; i++)
                cout << q[i] << " ";
            cout << endl;
        }
    }

    void makeEmpty() {
        rear = -1;
        cout << "Queue Emptied\n";
    }
};

class CircularQueue {
    int q[MAX], f, r;
public:
    CircularQueue() { f = r = -1; }

    void enqueue(int x) {
        if ((r + 1) % MAX == f)
            cout << "Overflow\n";
        else {
            if (f == -1) f = 0;
            r = (r + 1) % MAX;
            q[r] = x;
            cout << "Inserted\n";
        }
    }

    void dequeue() {
        if (f == -1)
            cout << "Underflow\n";
        else {
            cout << "Deleted: " << q[f] << endl;
            if (f == r)
                f = r = -1;
            else
                f = (f + 1) % MAX;
        }
    }

    void display() {
        if (f == -1)
            cout << "Empty\n";
        else {
            int i = f;
            while (true) {
                cout << q[i] << " ";
                if (i == r) break;
                i = (i + 1) % MAX;
            }
            cout << endl;
        }
    }

    void makeEmpty() {
        f = r = -1;
        cout << "Queue Emptied\n";
    }
};

int main() {
    StaticQueue sq;
    PriorityQueue pq;
    CircularQueue cq;

    int choice, op, val;

    do {
        cout << "\nNischaya Maharjan\n";
        cout << "1.Static Queue\n2.Priority Queue\n3.Circular Queue\n4.Exit\n";
        cin >> choice;

        switch (choice) {
        case 1:
            do {
                cout << "\nNischaya Maharjan\nStatic Queue\n";
                cout << "1.Enqueue\n2.Dequeue\n3.Display\n4.Make Empty\n5.Back\n";
                cin >> op;
                if (op == 1) { cin >> val; sq.enqueue(val); }
                else if (op == 2) sq.dequeue();
                else if (op == 3) sq.display();
                else if (op == 4) sq.makeEmpty();
            } while (op != 5);
            break;

        case 2:
            do {
                cout << "\nNischaya Maharjan\nPriority Queue\n";
                cout << "1.Enqueue\n2.Dequeue\n3.Display\n4.Make Empty\n5.Back\n";
                cin >> op;
                if (op == 1) { cin >> val; pq.enqueue(val); }
                else if (op == 2) pq.dequeue();
                else if (op == 3) pq.display();
                else if (op == 4) pq.makeEmpty();
            } while (op != 5);
            break;

        case 3:
            do {
                cout << "\nNischaya Maharjan\nCircular Queue\n";
                cout << "1.Enqueue\n2.Dequeue\n3.Display\n4.Make Empty\n5.Back\n";
                cin >> op;
                if (op == 1) { cin >> val; cq.enqueue(val); }
                else if (op == 2) cq.dequeue();
                else if (op == 3) cq.display();
                else if (op == 4) cq.makeEmpty();
            } while (op != 5);
            break;
        }
    } while (choice != 4);

    return 0;
}
